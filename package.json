    "nodejs",
    "typescript",
    "graphql",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.4.1",
  "dependencies": {
    "@types/node": "^22.15.21",
    "@types/pg": "^8.15.2",
    "dotenv": "^16.5.0",
    "graphql": "^16.11.0",
    "pg": "^8.16.0",
    "typescript": "^5.8.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.4",
    "tsx": "^4.19.4"
  }
}
